{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ecomm-dev": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "skipTests": true
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/ecomm-dev",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css",
              {
                "input": "./src/assets/css/bootstrap.min.css",
                "bundleName": "bootstrapmin",
                "inject": false
              },
              {
                "input": "./src/assets/css/font-awesome.min.css",
                "bundleName": "fontawesomemin",
                "inject": false
              },
              {
                "input": "./src/assets/css/owl.carousel.min.css",
                "bundleName": "owlcarouselmin",
                "inject": false
              },
              {
                "input": "./src/assets/css/owl.theme.default.min.css",
                "bundleName": "owlthemedefaultmin",
                "inject": false
              },
              {
                "input": "./src/assets/css/jquery.bxslider.min.css",
                "bundleName": "jquerybxslidermin",
                "inject": false
              },
              {
                "input": "./src/assets/css/magnific-popup.css",
                "bundleName": "magnificpopup",
                "inject": false
              },
              {
                "input": "./src/assets/css/rating.css",
                "bundleName": "rating",
                "inject": false
              },
              {
                "input": "./src/assets/css/spacing.css",
                "bundleName": "spacing",
                "inject": false
              },
              {
                "input": "./src/assets/css/bootstrap-touch-slider.css",
                "bundleName": "bootstraptouchslider",
                "inject": false
              },
              {
                "input": "./src/assets/css/animate.min.css",
                "bundleName": "animatemin",
                "inject": false
              },
              {
                "input": "./src/assets/css/tree-menu.css",
                "bundleName": "treemenu",
                "inject": false
              },
              {
                "input": "./src/assets/css/select2.min.css",
                "bundleName": "select2min",
                "inject": false
              },
              {
                "input": "./src/assets/css/main.css",
                "bundleName": "main",
                "inject": false
              },
              {
                "input": "./src/assets/css/responsive.css",
                "bundleName": "responsive",
                "inject": false
              }
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "ecomm-dev:build:production"
            },
            "development": {
              "browserTarget": "ecomm-dev:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ecomm-dev:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "tsconfig.spec.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    }
  }
}
